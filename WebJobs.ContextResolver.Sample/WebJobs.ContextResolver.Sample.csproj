  <ItemGroup>
    <ProjectReference Include="..\WebJobs.ContextResolver\WebJobs.ContextResolver.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>
  <ItemGroup>
